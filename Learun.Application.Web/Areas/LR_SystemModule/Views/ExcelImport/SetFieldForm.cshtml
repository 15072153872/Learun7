@{
    ViewBag.Title = "设置excel导入字段属性";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">设置字段<font face="宋体">*</font></div>
        <input id="F_Name" type="text" class="form-control" readonly />
    </div>
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">关联类型<font face="宋体">*</font></div>
        <div id="F_RelationType" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">数据选项</div>
        <div id="F_DataItemCode"></div>
    </div>
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">数据选项</div>
        <div id="F_DataSourceId"></div>
    </div>
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">固定数值</div>
        <input id="F_Value" type="text" class="form-control"/>
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_SystemModule/Views/ExcelImport/SetFieldForm.js")
